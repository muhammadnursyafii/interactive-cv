{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/backend/index.js"
        },
        {
            "source": "/((?!api/|.*\\..*).*)",
            "destination": "/index.html"
        }
    ],
    "builds": [
    { "src": "next.config.js", "use": "@vercel/next" }
  ]
}
